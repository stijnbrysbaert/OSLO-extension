@prefix :       <https://stijnbrysbaert.github.io/OSLO-extension/vocabulary.ttl#> .
@prefix adr:    <https://data.vlaanderen.be/ns/adres#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix mob:    <https://data.vlaanderen.be/ns/mobiliteit/trips-en-aanbod#> .
@prefix mv:     <http://schema.mobivoc.org/#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix trans:  <https://data.vlaanderen.be/ns/transportnetwerk#> .
@prefix rdf:    <https://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix tn:    <https://data.vlaanderen.be/ns/transportnetwerk#> .
@prefix vs:     <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd:    <https://www.w3.org/2001/XMLSchema#> .

:Station a owl:Class ;
         rdfs:label "station"@nl ;
         rdfs:comment "Station waar voertuigen kunnen worden geplaatst."@nl ;
         dct:created "2020-12-19"^^xsd:date ;
         dct:modified "2020-12-19"^^xsd:date ;
         vs:term_status "unstable"@en ;
         rdfs:subClassOf tn:Transportobject, dct:Location .

:geplaatst a rdf:Property ;
           rdfs:domain mob:Beschikbaarheid ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Is het object momenteel geplaatst op de straat?"@be-nl ;
           rdfs:label "geplaatst"@be-nl ;
           dct:created "2020-10-12"^^xsd:date ;
           dct:modified "2020-10-12"^^xsd:date ;
           vs:term_status "unstable"@en . 

:actief    a rdf:Property ;
           rdfs:domain mob:Beschikbaarheid ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Verhuurd het object momenteel voertuigen?"@be-nl ;
           rdfs:label "actief"@be-nl ;
           dct:created "2020-10-12"^^xsd:date ;
           dct:modified "2020-10-12"^^xsd:date ;
           vs:term_status "unstable"@en .

:ontvangt  a rdf:Property ;
           rdfs:domain mob:Beschikbaarheid ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Laat het station het terugbrengen van voertuigen toe?"@be-nl ;
           rdfs:label "ontvangt"@be-nl ;
           dct:created "2020-10-12"^^xsd:date ;
           dct:modified "2020-10-12"^^xsd:date ;
           vs:term_status "unstable"@en .

:voertuigTypesBeschikbaar a rdf:Property ;
                        rdfs:label "voertuigTypesBeschikbaar"@be-nl;
                        rdfs:comment "Beschikbaarheid van een bepaalt type voertuig."@be-nl ;
                        rdfs:domain mob:Beschikbaarheid ;
                        rdfs:range :VoertuigenBeschikbaar ;
                        dct:created "2020-12-18"^^xsd:date ;
                        dct:modified "2020-12-18"^^xsd:date ;
                        vs:term_status "unstable"@en .

:voertuigTypesDocks a rdf:Property ;
                        rdfs:label "voertuigTypesDocks"@be-nl;
                        rdfs:comment "Beschikbaarheid van het aantal plaatsen specifiek voor een bepaalt type voertuig."@be-nl ;
                        rdfs:domain mob:Beschikbaarheid ;
                        rdfs:range :VoertuigenDocks ;
                        dct:created "2020-12-18"^^xsd:date ;
                        dct:modified "2020-12-18"^^xsd:date ;
                        vs:term_status "unstable"@en .

:VoertuigenBeschikbaar  a owl:Class ;
                        rdfs:label "VoertuigenBeschikbaar"@be-nl;
                        rdfs:comment "Het aantal voertuigen dat beschikbaar is."@be-nl ;
                        dct:created "2020-10-12"^^xsd:date ;
                        dct:modified "2020-12-18"^^xsd:date ;
                        vs:term_status "unstable"@en .

:VoertuigenDocks        a owl:Class ;
                        rdfs:label "VoertuigenDocks"@be-nl;
                        rdfs:comment "Het aantal docks die beschikbaar zijn."@be-nl ;
                        dct:created "2020-10-12"^^xsd:date ;
                        dct:modified "2020-12-18"^^xsd:date ;
                        vs:term_status "unstable"@en .

:aantal a rdf:Property ;
        rdfs:label "aantal"@be-nl ;
        rdfs:comment "Aantal voertuigen van een bepaalt type."@be-nl ;
        rdfs:range xsd:integer ;
        rdfs:domain :VoertuigenDocks, :voertuigenBeschikbaar ;
        dct:created "2020-12-18"^^xsd:date ;
        dct:modified "2020-12-18"^^xsd:date ;
        vs:term_status "unstable"@en .

:voertuigenOnbruikbaar a rdf:Property ;
                       rdfs:label "voertuigen onbruikbaar"@be-nl ;
                       rdfs:comment "Het aantal onbruikbare voertuigen in een parkeer station."@be-nl ;
                       rdfs:domain mob:Beschikbaarheid ;
                       rdfs:range xsd:integer ;
                       dct:created "2020-10-12"^^xsd:date ;
                       dct:modified "2020-10-12"^^xsd:date ;
                       vs:term_status "unstable"@en .                      

:mobiliteitsdienst      a rdf:Property ;
                        rdfs:label "mobiliteitsdienst" ;
                        rdfs:comment "Mobiliteitsdienst welke dit transportobject invult."@be-nl ;
                        rdfs:domain mob:Transportobject ;
                        rdfs:range mob:Mobiliteitsdienst ;
                        dct:created "2020-10-12"^^xsd:date ;
                        dct:modified "2020-10-12"^^xsd:date ;
                        vs:term_status "unstable"@en .

:totaalPlaatsen a rdf:Property ;
                rdfs:label "totaal aantal plaatsen"@be-nl ;
                rdfs:comment "Het totaal aantal parkeerplaatsen in een object. Niet vereist wanneer er een ongelimiteerde parkeer capaciteit is."@be-nl ;
                rdfs:domain mob:Beschikbaarheid ;
                rdfs:range xsd:Number ;
                dct:created "2020-10-12"^^xsd:date ;
                dct:modified "2020-10-12"^^xsd:date ;
                vs:term_status "unstable"@en .

:plaatsenVrij   a rdf:Property ;                
                rdfs:label "aantal vrije plaatsen"@be-nl ;
                rdfs:comment "Het aantal plaatsen die vrij zijn om een voertuig in te plaatsen."@be-nl ;
                rdfs:domain mob:Beschikbaarheid ;
                rdfs:range xsd:Number ;
                dct:created "2020-10-18"^^xsd:date ;
                dct:modified "2020-10-18"^^xsd:date ;
                vs:term_status "unstable"@en .

:plaatsenOnbruikbaar a rdf:Property ;
                     rdfs:label "aantal onbruikbare plaatsen"@be-nl ;
                     rdfs:comment "Het aantal parkeerplaatsen die vrij maar niet bruikbaar zijn in een parkeer station."@be-nl ;
                     rdfs:domain mob:Beschikbaarheid ;
                     rdfs:range xsd:Number ;
                     dct:created "2020-10-12"^^xsd:date ;
                     dct:modified "2020-10-12"^^xsd:date ;
                     vs:term_status "unstable"@en .